{
    "name": "ipejasinovic/swoole-universal-pool",
    "type": "library",
    "description": "Universal DB Pool for Swoole",
    "keywords": ["swoole", "postgres", "mysql", "pool", "redis"],
    "require": {
        "php": ">=8.0",
        "ext-swoole": ">=5.0"
    },
    "authors": [
        {
            "name": "Igor Pejašinović",
            "email": "ipejasinovic@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swoole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwooleTest\\": "tests"
        }
    },
    "require-dev": {
        "swoole/ide-helper": ">=5.0",
        "phpunit/phpunit": "^7.5"
    }
}